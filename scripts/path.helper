#!/bin/bash

if ! which brew 1>/dev/null 2>/dev/null;
then
    jenkinks_home="/hom/jenkins"
    PATH="$jenkinks_home/bin:$jenkinks_home/.local/bin:$PATH"
    export PATH="$jenkinks_home/.linuxbrew/bin:$PATH"
    export PATH="/home/jenkins/.linuxbrew/sbin:$PATH"
    export MANPATH="$jenkinks_home/.linuxbrew/share/man:$MANPATH"
    export INFOPATH="$jenkinks_home/.linuxbrew/share/info:$INFOPATH"
fi
